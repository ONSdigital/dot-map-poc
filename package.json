{
  "name": "@onsvisual/sveltekit-starter",
  "homepage": "http://onsdigital.github.io/sveltekit-starter",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && node ./scripts/js-fix",
    "build:preview": "cross-env PUBLIC_APP_ENV=preview vite build",
    "data:download": "node ./scripts/download-data.js",
    "package": "vite package",
    "preview": "vite preview",
    "deploy": "gh-pages -d build -t",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "author": "Ahmad Barclay",
  "license": "MIT",
  "devDependencies": {
    "@onsvisual/svelte-components": "^1.0.22",
    "@onsvisual/svelte-maps": "^1.2.28",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.47.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@turf/bbox": "^7.2.0",
    "@turf/boolean-point-in-polygon": "^7.2.0",
    "@turf/random": "^7.2.0",
    "cross-env": "^7.0.3",
    "d3-dsv": "^3.0.1",
    "d3-quadtree": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "gh-pages": "^6.3.0",
    "point-in-polygon-hao": "^1.2.4",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^5.0.0",
    "topojson-client": "^3.1.0",
    "vite": "^5.4.4"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "type": "module"
}
